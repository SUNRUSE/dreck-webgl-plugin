{
  "name": "celluloid-engine",
  "description": "Simple cel-shaded games engine for your browser.",
  "version": "0.0.0",
  "engines": {
    "node": ">=16.11.1"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/celluloid-engine"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "license": "MIT",
  "devDependencies": {
    "@skitscript/types-nodejs": "2.0.0",
    "@types/jasmine": "3.10.3",
    "@types/node": "16.11.25",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "del-cli": "4.0.1",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jasmine": "4.0.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  },
  "scripts": {
    "buildClean": "del-cli \"**/*.js\" \"**/*.d.ts\" \"!**/node_modules/**/*\"",
    "buildLint": "eslint \"**/*.ts\"",
    "buildLintFix": "eslint --fix \"**/*.ts\"",
    "buildTypeScript": "tsc",
    "buildJasmine": "jasmine --config=jasmine.json",
    "buildNyc": "nyc --check-coverage --all --include \"src/**/*.js\" --exclude \"**/unit.*\" --lines 100 --functions 100 --branches 100 --statements 100 npm run-script buildJasmine",
    "test": "npm-run-all buildClean buildLintFix buildTypeScript buildNyc"
  },
  "files": [
    "src/**/index.js",
    "src/**/index.d.ts"
  ],
  "types": "src/index.d.ts",
  "sideEffects": false
}
